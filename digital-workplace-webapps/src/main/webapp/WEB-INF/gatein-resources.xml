<gatein-resources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.exoplatform.org/xml/ns/gatein_resources_1_4 http://www.exoplatform.org/xml/ns/gatein_resources_1_4"
  xmlns="http://www.exoplatform.org/xml/ns/gatein_resources_1_4">

  <portal-skin>
    <skin-name>Enterprise</skin-name>
    <skin-module>customModuleDigitalWorkplace</skin-module>
    <css-path>/css/main.css</css-path>
    <css-priority>11</css-priority>
  </portal-skin>

  <portlet>
    <name>digitalWorkplaceRecentDocuments</name>
    <module>
      <depends>
        <module>digitalWorkplaceRecentDocumentsBundle</module>
      </depends>
    </module>
  </portlet>

  <module>
    <name>digitalWorkplaceRecentDocumentsBundle</name>
    <script>
      <minify>false</minify>
      <path>/js/recentDocuments.bundle.js</path>
    </script>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>vuetify</module>
    </depends>
    <depends>
      <module>eXoVueI18n</module>
    </depends>
  </module>

  <portlet>
    <name>digitalWorkplaceSharedDocuments</name>
    <module>
      <depends>
        <module>digitalWorkplaceSharedDocumentsBundle</module>
      </depends>
    </module>
  </portlet>

  <module>
    <name>digitalWorkplaceSharedDocumentsBundle</name>
    <script>
      <minify>false</minify>
      <path>/js/sharedDocuments.bundle.js</path>
    </script>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>vuetify</module>
    </depends>
    <depends>
      <module>eXoVueI18n</module>
    </depends>
  </module>

  <portlet>
    <name>digitalWorkplaceFavoriteDocuments</name>
    <module>
      <depends>
        <module>digitalWorkplaceFavoriteDocumentsBundle</module>
      </depends>
    </module>
  </portlet>

  <module>
    <name>digitalWorkplaceFavoriteDocumentsBundle</name>
    <script>
      <minify>false</minify>
      <path>/js/favoriteDocuments.bundle.js</path>
    </script>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>vuetify</module>
    </depends>
    <depends>
      <module>eXoVueI18n</module>
    </depends>
  </module>

  <portlet>
    <name>digitalWorkplaceEvents</name>
    <module>
      <depends>
        <module>digitalWorkplaceEventsBundle</module>
      </depends>
    </module>
  </portlet>

  <module>
    <name>digitalWorkplaceEventsBundle</name>
    <script>
      <minify>false</minify>
      <path>/js/events.bundle.js</path>
    </script>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>vuetify</module>
    </depends>
    <depends>
      <module>eXoVueI18n</module>
    </depends>
    <depends>
      <module>calendarEvent</module>
    </depends>
  </module>

  <portlet>
    <name>digitalWorkplaceTasks</name>
    <module>
      <depends>
        <module>digitalWorkplaceTasksBundle</module>
      </depends>
    </module>
  </portlet>

  <module>
    <name>digitalWorkplaceTasksBundle</name>
    <script>
      <minify>false</minify>
      <path>/js/tasks.bundle.js</path>
    </script>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>vuetify</module>
    </depends>
    <depends>
      <module>eXoVueI18n</module>
    </depends>
    <depends>
      <module>commons-editor</module>
      <as>editor</as>
    </depends>
    <depends>
      <module>jquery</module>
      <as>$</as>
    </depends>
  </module>
</gatein-resources>
