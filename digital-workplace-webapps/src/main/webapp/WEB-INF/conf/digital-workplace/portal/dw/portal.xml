<?xml version="1.0" encoding="UTF-8"?>
<portal-config>
    <portal-name>dw</portal-name>
    <locale>en</locale>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>*:/platform/administrators</edit-permission>
    <properties>
        <entry key="sessionAlive">onDemand</entry>
        <entry key="showPortletInfo">0</entry>
    </properties>

    <portal-layout>
        <container id="NavigationBody" template="system:/groovy/portal/webui/container/UILeftNavigationContainer.gtmpl">
            <access-permissions>Everyone</access-permissions>
            <container id="DWLeftNavigation" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
                <access-permissions>*:/platform/users</access-permissions>
                <container id="DWLeftNavigationPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
                    <access-permissions>*:/platform/users</access-permissions>
                    <portlet-application>
                        <portlet>
                            <application-ref>digital-workplace</application-ref>
                            <portlet-ref>digitalWorkplaceLeftNavigation</portlet-ref>
                        </portlet>
                        <access-permissions>*:/platform/users</access-permissions>
                        <show-info-bar>false</show-info-bar>
                    </portlet-application>
                </container>

                <container id="DWAdministrationPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
                    <access-permissions>*:/platform/users</access-permissions>
                    <portlet-application>
                        <portlet>
                            <application-ref>digital-workplace</application-ref>
                            <portlet-ref>digitalWorkplaceAdministration</portlet-ref>
                        </portlet>
                        <access-permissions>*:/platform/users</access-permissions>
                        <show-info-bar>false</show-info-bar>
                    </portlet-application>
                </container>

                <container id="DWSpacesNavigationPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
                    <access-permissions>*:/platform/users</access-permissions>
                    <portlet-application>
                        <portlet>
                            <application-ref>digital-workplace</application-ref>
                            <portlet-ref>digitalWorkplaceSpacesNavigation</portlet-ref>
                        </portlet>
                        <access-permissions>*:/platform/users</access-permissions>
                        <show-info-bar>false</show-info-bar>
                    </portlet-application>
                </container>

                <container id="DWMyAccountPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
                    <access-permissions>*:/platform/users</access-permissions>
                    <portlet-application>
                        <portlet>
                            <application-ref>digital-workplace</application-ref>
                            <portlet-ref>digitalWorkplaceMyAccount</portlet-ref>
                        </portlet>
                        <access-permissions>*:/platform/users</access-permissions>
                        <show-info-bar>false</show-info-bar>
                    </portlet-application>
                </container>

                <container id="DWMySettingsPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
                    <access-permissions>*:/platform/users</access-permissions>
                    <portlet-application>
                        <portlet>
                            <application-ref>digital-workplace</application-ref>
                            <portlet-ref>digitalWorkplaceMySettings</portlet-ref>
                        </portlet>
                        <access-permissions>*:/platform/users</access-permissions>
                        <show-info-bar>false</show-info-bar>
                    </portlet-application>
                </container>

            </container>
            <container id="DWRightBody" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
                <access-permissions>Everyone</access-permissions>
                <page-body> </page-body>
            </container>
        </container>
    </portal-layout>
</portal-config>
