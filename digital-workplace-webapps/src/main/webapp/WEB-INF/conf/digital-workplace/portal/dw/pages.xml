<?xml version="1.0" encoding="UTF-8"?>
<page-set xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_7 http://www.gatein.org/xml/ns/gatein_objects_1_7"
  xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_7">
  <page>
    <name>homepage</name>
    <title>Home Page</title>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>*:/platform/administrators</edit-permission>
    <container id="digitalWorkplaceHomePage"
      template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
      <access-permissions>*:/platform/users</access-permissions>
      <container id="SliderContainer"
        template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
        <access-permissions>*:/platform/users</access-permissions>
        <portlet-application>
          <portlet>
            <application-ref>digital-workplace</application-ref>
            <portlet-ref>digitalWorkplaceSlider</portlet-ref>
          </portlet>
          <title>Slider</title>
          <access-permissions>*:/platform/users</access-permissions>
          <show-info-bar>false</show-info-bar>
        </portlet-application>
      </container>
      <container id="MiddleContainer"
        template="system:/groovy/portal/webui/container/UIResponsiveTableColumnContainer.gtmpl">
        <access-permissions>*:/platform/users</access-permissions>
        <container id="LatestNewsContainer"
          template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <portlet-application>
            <portlet>
              <application-ref>digital-workplace</application-ref>
              <portlet-ref>digitalWorkplaceNews</portlet-ref>
            </portlet>
            <title>News</title>
            <access-permissions>*:/platform/users</access-permissions>
            <show-info-bar>false</show-info-bar>
          </portlet-application>
        </container>
        <container id="UserProfileContainer"
          template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <container id="ProfileStatsContainer"
            template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
            <access-permissions>*:/platform/users</access-permissions>
            <portlet-application>
              <portlet>
                <application-ref>digital-workplace</application-ref>
                <portlet-ref>digitalWorkplaceProfileStats</portlet-ref>
              </portlet>
              <title>Profile stats</title>
              <access-permissions>*:/platform/users</access-permissions>
              <show-info-bar>false</show-info-bar>
            </portlet-application>
          </container>
          <container id="PerkstoreWalletContainer"
            template="system:/groovy/portal/webui/container/UIResponsiveTableColumnContainer.gtmpl">
            <access-permissions>*:/platform/users</access-permissions>
            <container id="PerkstoreContainer"
              template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
              <access-permissions>*:/platform/users</access-permissions>
              <portlet-application>
                <portlet>
                  <application-ref>digital-workplace</application-ref>
                  <portlet-ref>digitalWorkplacePerkstore</portlet-ref>
                </portlet>
                <title>Perkstore</title>
                <access-permissions>*:/platform/users</access-permissions>
                <show-info-bar>false</show-info-bar>
              </portlet-application>
            </container>
            <container id="WalletContainer"
              template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
              <access-permissions>*:/platform/users</access-permissions>
              <portlet-application>
                <portlet>
                  <application-ref>digital-workplace</application-ref>
                  <portlet-ref>digitalWorkplaceWallet</portlet-ref>
                </portlet>
                <title>Wallet</title>
                <access-permissions>*:/platform/users</access-permissions>
                <show-info-bar>false</show-info-bar>
              </portlet-application>
            </container>
          </container>
        </container>
      </container>

      <container id="BottomContainer"
        template="system:/groovy/portal/webui/container/UIResponsiveTableColumnContainer.gtmpl">
        <access-permissions>*:/platform/users</access-permissions>
        <container id="EventsContainer"
          template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <portlet-application>
            <portlet>
              <application-ref>digital-workplace</application-ref>
              <portlet-ref>digitalWorkplaceEvents</portlet-ref>
            </portlet>
            <title>Events</title>
            <access-permissions>*:/platform/users</access-permissions>
            <show-info-bar>false</show-info-bar>
          </portlet-application>
        </container>
        <container id="TasksContainer"
          template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <portlet-application>
            <portlet>
              <application-ref>digital-workplace</application-ref>
              <portlet-ref>digitalWorkplaceTasks</portlet-ref>
            </portlet>
            <title>Tasks</title>
            <access-permissions>*:/platform/users</access-permissions>
            <show-info-bar>false</show-info-bar>
          </portlet-application>
        </container>
        <container id="DocumentsContainer"
          template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <container
            template="system:/groovy/portal/webui/container/UIDWTabContainer.gtmpl">
            <factory-id>TabContainer</factory-id>
            <container
              template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
              <title>recentDocuments</title>
              <access-permissions>*:/platform/users</access-permissions>
              <portlet-application>
                <portlet>
                  <application-ref>digital-workplace</application-ref>
                  <portlet-ref>digitalWorkplaceRecentDocuments</portlet-ref>
                </portlet>
                <title>Recent documents</title>
                <access-permissions>*:/platform/users</access-permissions>
                <show-info-bar>false</show-info-bar>
              </portlet-application>
            </container>
            <container
              template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
              <title>sharedDocuments</title>
              <access-permissions>*:/platform/users</access-permissions>
              <portlet-application>
                <portlet>
                  <application-ref>digital-workplace</application-ref>
                  <portlet-ref>digitalWorkplaceSharedDocuments</portlet-ref>
                </portlet>
                <title>Shared with me documents</title>
                <access-permissions>*:/platform/users</access-permissions>
                <show-info-bar>false</show-info-bar>
              </portlet-application>
            </container>
            <container
              template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
              <title>favoriteDocuments</title>
              <access-permissions>*:/platform/users</access-permissions>
              <portlet-application>
                <portlet>
                  <application-ref>digital-workplace</application-ref>
                  <portlet-ref>digitalWorkplaceFavoriteDocuments</portlet-ref>
                </portlet>
                <title>Favorite documents</title>
                <access-permissions>*:/platform/users</access-permissions>
                <show-info-bar>false</show-info-bar>
              </portlet-application>
            </container>
          </container>
        </container>
      </container>
    </container>
  </page>

  <page>
    <name>profile</name>
    <title>Profile</title>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>manager:/platform/administrators</edit-permission>

    <container id="UIUserNavigationPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
      <access-permissions>*:/platform/users</access-permissions>
      <portlet-application>
        <portlet>
          <application-ref>platformNavigation</application-ref>
          <portlet-ref>UIUserNavigationPortlet</portlet-ref>
        </portlet>
        <title>User Navigation Portlet</title>
        <access-permissions>*:/platform/users</access-permissions>
        <show-info-bar>false</show-info-bar>
        <show-application-state>true</show-application-state>
      </portlet-application>
    </container>

    <container id="socialMainLayout" template="system:/groovy/portal/webui/container/UITableColumnContainer.gtmpl">
      <access-permissions>*:/platform/users</access-permissions>

      <container id="LeftColumnContainer" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
        <access-permissions>*:/platform/users</access-permissions>
        <container id="BasicProfilePortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <portlet-application>
            <portlet>
              <application-ref>social-portlet</application-ref>
              <portlet-ref>BasicProfilePortlet</portlet-ref>
            </portlet>
            <title>Profile</title>
            <access-permissions>*:/platform/users</access-permissions>
            <show-info-bar>false</show-info-bar>
            <show-application-state>true</show-application-state>
          </portlet-application>
        </container>
        <container id="left-profile-container"
                   template="system:/groovy/portal/webui/container/UIAddOnColumnContainer.gtmpl">
          <name>left-profile-container</name>
          <access-permissions>*:/platform/users</access-permissions>
          <factory-id>addonContainer</factory-id>
        </container>
      </container>

      <container id="CenterColumnContainer" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
        <access-permissions>*:/platform/users</access-permissions>
        <container id="ExperienceProfilePortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <portlet-application>
            <portlet>
              <application-ref>social-portlet</application-ref>
              <portlet-ref>ExperienceProfilePortlet</portlet-ref>
            </portlet>
            <title>Experience Profile Portlet</title>
            <access-permissions>*:/platform/users</access-permissions>
            <show-info-bar>false</show-info-bar>
            <show-application-state>true</show-application-state>
          </portlet-application>
        </container>

        <container id="middle-profile-container"
                   template="system:/groovy/portal/webui/container/UIAddOnColumnContainer.gtmpl">
          <name>middle-profile-container</name>
          <access-permissions>*:/platform/users</access-permissions>
          <factory-id>addonContainer</factory-id>
        </container>

        <container id="RecentActivitiesPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <portlet-application>
            <portlet>
              <application-ref>social-portlet</application-ref>
              <portlet-ref>RecentActivitiesPortlet</portlet-ref>
            </portlet>
            <title>Recent Activities Portlet</title>
            <access-permissions>*:/platform/users</access-permissions>
            <show-info-bar>false</show-info-bar>
            <show-application-state>true</show-application-state>
          </portlet-application>
        </container>
      </container>

      <container id="RightColumnContainer" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
        <access-permissions>*:/platform/users</access-permissions>

        <container id="righttop-profile-container"
                   template="system:/groovy/portal/webui/container/UIAddOnColumnContainer.gtmpl">
          <name>righttop-profile-container</name>
          <access-permissions>*:/platform/users</access-permissions>
          <factory-id>addonContainer</factory-id>
        </container>

        <container id="MiniConnectionsPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <portlet-application>
            <portlet>
              <application-ref>social-portlet</application-ref>
              <portlet-ref>MiniConnectionsPortlet</portlet-ref>
            </portlet>
            <title>Connections User Portlet</title>
            <access-permissions>*:/platform/users</access-permissions>
            <show-info-bar>false</show-info-bar>
            <show-application-state>true</show-application-state>
          </portlet-application>
        </container>

        <container id="rightbottom-profile-container"
                   template="system:/groovy/portal/webui/container/UIAddOnColumnContainer.gtmpl">
          <name>rightbottom-profile-container</name>
          <access-permissions>*:/platform/users</access-permissions>
          <factory-id>addonContainer</factory-id>
        </container>
      </container>
    </container>
  </page>

  <page>
    <name>wallet</name>
    <title>Wallet</title>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>*:/platform/administrators</edit-permission>
    <container id="UIUserNavigationPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
      <access-permissions>*:/platform/users</access-permissions>
      <portlet-application>
        <portlet>
          <application-ref>platformNavigation</application-ref>
          <portlet-ref>UIUserNavigationPortlet</portlet-ref>
        </portlet>
        <title>User Navigation Portlet</title>
        <access-permissions>*:/platform/users</access-permissions>
        <show-info-bar>false</show-info-bar>
        <show-application-state>true</show-application-state>
      </portlet-application>
    </container>
    <container id="WalletParentContainer" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
      <access-permissions>*:/platform/users</access-permissions>
      <container id="top-wallet-container" template="system:/groovy/portal/webui/container/UIAddOnContainer.gtmpl">
        <name>top-wallet-container</name>
        <access-permissions>*:/platform/users</access-permissions>
        <factory-id>addonContainer</factory-id>
      </container>
      <container id="WalletContainer" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
        <access-permissions>*:/platform/users</access-permissions>
        <portlet-application>
          <portlet>
            <application-ref>wallet</application-ref>
            <portlet-ref>Wallet</portlet-ref>
            <preferences>
              <preference>
                <name>walletAppMaximize</name>
                <value>true</value>
              </preference>
            </preferences>
          </portlet>
          <title>Wallet</title>
          <access-permissions>*:/platform/users</access-permissions>
          <show-info-bar>false</show-info-bar>
        </portlet-application>
      </container>
      <container id="bottom-wallet-container" template="system:/groovy/portal/webui/container/UIAddOnContainer.gtmpl">
        <name>bottom-wallet-container</name>
        <access-permissions>*:/platform/users</access-permissions>
        <factory-id>addonContainer</factory-id>
      </container>
    </container>
  </page>

  <page>
    <name>achievements</name>
    <title>Gamification Achievements</title>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>*:/platform/administrators</edit-permission>
    <container id="UIUserNavigationPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
      <access-permissions>*:/platform/users</access-permissions>
      <portlet-application>
        <portlet>
          <application-ref>platformNavigation</application-ref>
          <portlet-ref>UIUserNavigationPortlet</portlet-ref>
        </portlet>
        <title>User Navigation Portlet</title>
        <access-permissions>*:/platform/users</access-permissions>
        <show-info-bar>false</show-info-bar>
        <show-application-state>true</show-application-state>
      </portlet-application>
    </container>
    <container id="GamificationParentContainer" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
      <access-permissions>*:/platform/users</access-permissions>
      <container id="top-Gamification-container" template="system:/groovy/portal/webui/container/UIAddOnContainer.gtmpl">
        <name>top-Gamification-container</name>
        <access-permissions>*:/platform/users</access-permissions>
        <factory-id>addonContainer</factory-id>
      </container>
      <container id="GamificationContainer" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
        <access-permissions>*:/platform/users</access-permissions>
        <portlet-application>
          <portlet>
            <application-ref>gamification-portlets</application-ref>
            <portlet-ref>GamificationInformationsboard</portlet-ref>
            <preferences>
              <preference>
                <name>gamificationAppMaximize</name>
                <value>true</value>
              </preference>
            </preferences>
          </portlet>
          <title>Wallet</title>
          <access-permissions>*:/platform/users</access-permissions>
          <show-info-bar>false</show-info-bar>
        </portlet-application>
      </container>
      <container id="bottom-gamification-container" template="system:/groovy/portal/webui/container/UIAddOnContainer.gtmpl">
        <name>bottom-wallet-container</name>
        <access-permissions>*:/platform/users</access-permissions>
        <factory-id>addonContainer</factory-id>
      </container>
    </container>
  </page>

  <page>
    <name>notifications</name>
    <title>Notifications Settings</title>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>manager:/platform/administrators</edit-permission>
    <container id="UIUserNavigationPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
      <access-permissions>*:/platform/users</access-permissions>
      <portlet-application>
        <portlet>
          <application-ref>platformNavigation</application-ref>
          <portlet-ref>UIUserNavigationPortlet</portlet-ref>
        </portlet>
        <title>User Navigation Portlet</title>
        <access-permissions>*:/platform/users</access-permissions>
        <show-info-bar>false</show-info-bar>
        <show-application-state>true</show-application-state>
      </portlet-application>
    </container>
    <container id="NotificationSettingsPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
      <access-permissions>*:/platform/users</access-permissions>
      <portlet-application>
        <portlet>
          <application-ref>social-juzu</application-ref>
          <portlet-ref>NotificationSettingsPortlet</portlet-ref>
        </portlet>
        <title>Notifications Settings</title>
        <access-permissions>*:/platform/users</access-permissions>
        <show-info-bar>false</show-info-bar>
        <show-application-state>true</show-application-state>
      </portlet-application>
    </container>
  </page>

  <page>
    <name>all-spaces</name>
    <title>All Spaces</title>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>manager:/platform/administrators</edit-permission>
    <container id="sociallayout" template="system:/groovy/portal/webui/container/UITableColumnContainer.gtmpl">
      <access-permissions>*:/platform/users</access-permissions>
      <container id="AllSpacesPortlet" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
        <access-permissions>*:/platform/users</access-permissions>
        <portlet-application>
          <portlet>
            <application-ref>social-portlet</application-ref>
            <portlet-ref>AllSpacesPortlet</portlet-ref>
          </portlet>
          <title>All Spaces</title>
          <access-permissions>*:/platform/users</access-permissions>
          <show-info-bar>false</show-info-bar>
          <show-application-state>true</show-application-state>
        </portlet-application>
      </container>
    </container>
  </page>

  <page>
    <name>account</name>
    <title>Account Settings</title>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>manager:/platform/administrators</edit-permission>
    <container id="digitalWorkplaceAccountPage"
               template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
    <access-permissions>*:/platform/users</access-permissions>
    </container>
  </page>

</page-set>