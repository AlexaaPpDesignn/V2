<portlet-app version="2.0" xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">

	<portlet>
		<portlet-name>digitalWorkplaceSlider</portlet-name>
		<portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
		<init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/slider.html</value>
    </init-param>
    <init-param>
      <description>List of profiles that can be used to enable this portlet</description>
      <name>exo.profiles</name>
      <value>ecms</value>
    </init-param>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Digital Workplace Slider</title>
		</portlet-info>
	</portlet>

